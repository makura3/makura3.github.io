<header class="page-header">
  <div class="header-list">
      <nav class="header-nav">
        <a id="main-nav-toggle" class="nav-icon"></a>
        <% for (var i in theme.menu){ %>
          <a class="header-nav-link" href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
        <% } %>
      </nav>
      <nav class="header-nav">
        <% if (theme.rss){ %>
          <a class="nav-icon" href="<%- url_for(theme.rss) %>" title="<%= __('rss_feed') %>"></a>
        <% } %>
        <a class="nav-icon" title="<%= __('search') %>"></a>
      </nav>
      <!-- <div id="search-form-wrap">
        <%- search_form({button: '&#xF002;'}) %>
      </div> -->
    </div>
  <div class="header-title">
    <a href="<%- url_for() %>" class="header-title-link">
      <h1 class="title">
      <%= config.title %>
      </h1>
    </a>
    <% if (config.subtitle){ %>
      <p class="subtitle">
        <%= config.subtitle %>
      </p>
    <% } %>
  </div>
</header>